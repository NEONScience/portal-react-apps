{
  "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "rules": {
    "linebreak-style": 0,
    "react/jsx-props-no-spreading": 0,
    "eslint linebreak-style": [0, "error", "windows"]
  }
}
