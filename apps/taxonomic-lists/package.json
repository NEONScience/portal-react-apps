{
  "name": "taxonomic-lists",
  "version": "2.10.0",
  "private": true,
  "homepage": "http://localhost/taxonomic-lists/",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "~4.0.0-alpha.61",
    "@material-ui/pickers": "^3.3.11",
    "@material-ui/styles": "^4.11.5",
    "bootstrap": "^3.4.1",
    "core-js": "^3.37.1",
    "datatables.net": "^1.13.11",
    "datatables.net-bs": "^1.13.11",
    "datatables.net-responsive": "^2.5.1",
    "datatables.net-responsive-bs": "^2.5.1",
    "jquery": "~3.7.1",
    "lodash": "^4.17.21",
    "portal-core-components": "github:NEONScience/portal-core-components#v2.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "whatwg-fetch": "^3.6.20"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:sync-assets": "node ../../node_modules/portal-core-components/scripts/sync-assets.js --prettify-index --cwd-relative-public-dir-path=\"public\"",
    "lint": "(npx eslint src/ --ext .js,.jsx,.ts,.tsx || true) && exit 0",
    "lint:fix": "(npx eslint --fix src/ --ext .js,.jsx,.ts,.tsx || true) && exit 0",
    "checks": "npm run build:sync-assets && yarn run lint && yarn run build",
    "checks:docker": "npm run build:docker-sync-assets && npm run lint:docker && npm run build:docker",
    "start:docker": "docker run --rm --tty --interactive --workdir=/app/apps/taxonomic-lists --publish=3001:3001 portal-react-apps/taxonomic-lists:latest-builder-base yarn run start",
    "start:docker-prod": "docker run --rm --tty --interactive --volume=/$(pwd)/build:/app/apps/taxonomic-lists/build --workdir=/app/apps/taxonomic-lists --publish=3000:3000 portal-react-apps/taxonomic-lists:latest-builder-base yarn run start:prod",
    "build:docker": "docker run --rm --tty --interactive --volume=/$(pwd)/build:/app/apps/taxonomic-lists/build --workdir=/app/apps/taxonomic-lists portal-react-apps/taxonomic-lists:latest-builder-base yarn run build",
    "lint:docker": "docker run --rm --tty --interactive --workdir=/app/apps/taxonomic-lists portal-react-apps/taxonomic-lists:latest-builder-base yarn run lint",
    "build:docker-sync-assets": "docker run --rm --tty --interactive --volume=/$(pwd)/public:/app/apps/taxonomic-lists/public --workdir=/app/apps/taxonomic-lists portal-react-apps/taxonomic-lists:latest-builder-base npm run build:sync-assets"
  },
  "devDependencies": {
    "commander": "^12.1.0",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
